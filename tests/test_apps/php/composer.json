{
  "name": "kreuzberg-dev/html-to-markdown-test-app",
  "description": "Test app for html-to-markdown PHP package",
  "require": {
    "php": ">=8.2",
    "kreuzberg-dev/html-to-markdown": ">=2.22.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../../packages/php",
      "options": {
        "symlink": false
      }
    }
  ],
  "config": {
    "platform": {
      "php": "8.4.16"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
